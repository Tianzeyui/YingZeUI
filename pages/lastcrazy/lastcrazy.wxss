
/*start*/
.fq-list {
  width: 100%;
  background: white;
}

.fq-item {
  position: relative;
  width: 374rpx;
  height: 475rpx;
  float: left;
  font-size: 25rpx;
}

.odd {
  margin-right: 2rpx;
}

.fq-item .pic {
  width: 100%;
  height: 375rpx;
}

.fq-item .logo {
  position: absolute;
  width: 110rpx;
  height: 60rpx;
  top: 350rpx;
  right: 40rpx;
  z-index: 20;
}

.fq-item .logo image {
  width: 100%;
  height: 100%;
}

.item-text {
  height: 100rpx;
  font-size: 28rpx;
}
.item-text text {
  display: block;
  margin-left: 10rpx;
  color: red;
}
.item-text view {
  margin-left: 10rpx;
  color: black;
}

.bt-text text {  
  float: right;
  color: #bebebe;
}
/*end*/








